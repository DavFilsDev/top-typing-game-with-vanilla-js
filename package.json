{
  "name": "typing-website-template",
  "version": "1.0.0",
  "description": "A barebones typing website for you to complete. ",
  "main": "index.html",
  "type": "module",
  "scripts": {
    "install:all": "npm install && npx tailwindcss init",
    "dev:index": "tailwindcss -i ./src/assets/css/input/index-input.css -o ./src/assets/css/output/index-output.css --watch",
    "dev:login": "tailwindcss -i ./src/assets/css/input/login-input.css -o ./src/assets/css/output/login-output.css --watch",
    "dev:home": "tailwindcss -i ./src/assets/css/input/home-input.css -o ./src/assets/css/output/home-output.css --watch",
    "build": "tailwindcss -i ./src/assets/css/global.css -o ./dist/global.css --minify"
  },
  "dependencies": {
    "tailwindcss": "^3.4.0"
  },
  "overrides": {
    "caniuse-lite": "latest"
  },
  "keywords": [
    "typing",
    "game",
    "speedtyping"
  ],
  "author": "HEI Madagascar : Toky & Fiantso",
  "license": "ISC"
}
